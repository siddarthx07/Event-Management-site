{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\n\n// Let's try with just one image first for debugging\nconst TEST_IMAGE = '/assets/events/pexels-zhuhehuai-716276.jpg';\nexport default {\n  name: 'LoginPage',\n  data() {\n    return {\n      email: '',\n      password: '',\n      rememberMe: false,\n      isLoading: false,\n      emailError: false,\n      passwordError: false\n    };\n  },\n  computed: {\n    backgroundStyle() {\n      return {\n        backgroundImage: `url(${TEST_IMAGE})`,\n        backgroundSize: 'cover',\n        backgroundPosition: 'center',\n        backgroundRepeat: 'no-repeat',\n        width: '100%',\n        height: '100vh'\n      };\n    }\n  },\n  methods: {\n    async login() {\n      this.isLoading = true;\n      this.emailError = false;\n      this.passwordError = false;\n      try {\n        const response = await axios.post('http://localhost:3000/api/auth/login', {\n          email: this.email,\n          password: this.password\n        });\n        const token = response.data.token;\n        const username = response.data.username;\n        localStorage.setItem('token', token);\n        localStorage.setItem('username', username);\n        if (this.rememberMe) {\n          localStorage.setItem('rememberMe', true);\n        } else {\n          localStorage.removeItem('rememberMe');\n        }\n        this.$root.token = token;\n        this.$root.username = username;\n        this.$router.push('/dashboard');\n      } catch (error) {\n        console.error('Login error:', error);\n        if (error.response && error.response.status === 401) {\n          this.passwordError = true;\n          alert('Incorrect email or password.');\n        } else {\n          alert('An unexpected error occurred. Please try again.');\n        }\n      } finally {\n        this.isLoading = false;\n      }\n    }\n  },\n  mounted() {\n    // Add this to check if the component is properly mounted\n    console.log('Component mounted');\n\n    // Add this to check the computed style\n    console.log('Background style:', this.backgroundStyle);\n\n    // Check if the image exists\n    const img = new Image();\n    img.onload = () => {\n      console.log('Image loaded successfully');\n    };\n    img.onerror = () => {\n      console.error('Image failed to load');\n    };\n    img.src = TEST_IMAGE;\n  }\n};","map":{"version":3,"names":["axios","TEST_IMAGE","name","data","email","password","rememberMe","isLoading","emailError","passwordError","computed","backgroundStyle","backgroundImage","backgroundSize","backgroundPosition","backgroundRepeat","width","height","methods","login","response","post","token","username","localStorage","setItem","removeItem","$root","$router","push","error","console","status","alert","mounted","log","img","Image","onload","onerror","src"],"sources":["/Users/siddarthbandi/Desktop/SID/project/event-management-platform/event-management-frontend/src/components/Login.vue"],"sourcesContent":["<template>\n  <div \n    class=\"login-container\" \n    :style=\"backgroundStyle\"\n  >\n    <div class=\"overlay\">\n      <div class=\"login-card\">\n        <h2>Sign In</h2>\n        <form @submit.prevent=\"login\">\n          <div class=\"form-group\" :class=\"{ 'has-error': emailError }\">\n            <label for=\"email\">Email</label>\n            <input type=\"email\" v-model=\"email\" id=\"email\" required />\n            <span v-if=\"emailError\" class=\"error-message\">Invalid email address.</span>\n          </div>\n          <div class=\"form-group\" :class=\"{ 'has-error': passwordError }\">\n            <label for=\"password\">Password</label>\n            <input type=\"password\" v-model=\"password\" id=\"password\" required />\n            <span v-if=\"passwordError\" class=\"error-message\">Incorrect password.</span>\n          </div>\n          <div class=\"form-group\">\n            <input type=\"checkbox\" v-model=\"rememberMe\" id=\"rememberMe\">\n            <label for=\"rememberMe\">Remember Me</label>\n          </div>\n          <button type=\"submit\" class=\"login-button\" :disabled=\"isLoading\">\n            <span v-if=\"!isLoading\">Login</span>\n            <span v-else class=\"spinner\"></span>\n          </button>\n        </form>\n        <p>Don't have an account? <router-link to=\"/register\">Sign Up</router-link></p>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\n// Let's try with just one image first for debugging\nconst TEST_IMAGE = '/assets/events/pexels-zhuhehuai-716276.jpg';\n\nexport default {\n  name: 'LoginPage',\n  data() {\n    return {\n      email: '',\n      password: '',\n      rememberMe: false,\n      isLoading: false,\n      emailError: false,\n      passwordError: false,\n    };\n  },\n  computed: {\n    backgroundStyle() {\n      return {\n        backgroundImage: `url(${TEST_IMAGE})`,\n        backgroundSize: 'cover',\n        backgroundPosition: 'center',\n        backgroundRepeat: 'no-repeat',\n        width: '100%',\n        height: '100vh',\n      };\n    }\n  },\n  methods: {\n    async login() {\n      this.isLoading = true;\n      this.emailError = false;\n      this.passwordError = false;\n      try {\n        const response = await axios.post('http://localhost:3000/api/auth/login', {\n          email: this.email,\n          password: this.password,\n        });\n\n        const token = response.data.token;\n        const username = response.data.username;\n\n        localStorage.setItem('token', token);\n        localStorage.setItem('username', username);\n\n        if (this.rememberMe) {\n          localStorage.setItem('rememberMe', true);\n        } else {\n          localStorage.removeItem('rememberMe');\n        }\n\n        this.$root.token = token;\n        this.$root.username = username;\n\n        this.$router.push('/dashboard');\n      } catch (error) {\n        console.error('Login error:', error);\n        if (error.response && error.response.status === 401) {\n          this.passwordError = true;\n          alert('Incorrect email or password.');\n        } else {\n          alert('An unexpected error occurred. Please try again.');\n        }\n      } finally {\n        this.isLoading = false;\n      }\n    },\n  },\n  mounted() {\n    // Add this to check if the component is properly mounted\n    console.log('Component mounted');\n    \n    // Add this to check the computed style\n    console.log('Background style:', this.backgroundStyle);\n    \n    // Check if the image exists\n    const img = new Image();\n    img.onload = () => {\n      console.log('Image loaded successfully');\n    };\n    img.onerror = () => {\n      console.error('Image failed to load');\n    };\n    img.src = TEST_IMAGE;\n  }\n};\n</script>\n\n<style scoped>\n.login-container {\n  position: relative;\n  min-height: 100vh;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n\n.overlay {\n  position: absolute;\n  top: 0;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  background: linear-gradient(to bottom right, rgba(0, 0, 0, 0.5), rgba(0, 0, 0, 0.3));\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  width: 100%;\n  height: 100%;\n}\n\n.login-card {\n  background-color: rgba(255, 255, 255, 0.7);\n  padding: 40px;\n  border-radius: 10px;\n  box-shadow: 0 8px 16px rgba(0, 0, 0, 0.3);\n  max-width: 400px;\n  width: 90%;\n  text-align: center;\n  animation: fadeIn 1s ease-in-out;\n  color: #333;\n  position: relative;\n  z-index: 1;\n}\n\n.form-group {\n  margin-bottom: 15px;\n  text-align: left;\n}\n\n.form-group.has-error input {\n  border-color: #e74c3c;\n}\n\n.form-group label {\n  display: block;\n  margin-bottom: 5px;\n  color: #555;\n  font-weight: 600;\n}\n\n.form-group input {\n  width: 100%;\n  padding: 10px;\n  border: 1px solid #ccc;\n  border-radius: 5px;\n  transition: border-color 0.3s ease;\n}\n\n.login-button {\n  width: 100%;\n  padding: 12px;\n  background-color: #6c63ff;\n  color: #fff;\n  border: none;\n  border-radius: 5px;\n  font-size: 1rem;\n  cursor: pointer;\n  transition: background-color 0.3s ease;\n}\n\n.login-button:disabled {\n  background-color: #a9a9a9;\n  cursor: not-allowed;\n}\n\n@keyframes fadeIn {\n  from {\n    opacity: 0;\n    transform: translateY(-20px);\n  }\n  to {\n    opacity: 1;\n    transform: translateY(0);\n  }\n}\n\n@media (max-width: 480px) {\n  .login-card {\n    padding: 20px;\n  }\n}\n</style>"],"mappings":";AAmCA,OAAOA,KAAI,MAAO,OAAO;;AAEzB;AACA,MAAMC,UAAS,GAAI,4CAA4C;AAE/D,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,UAAU,EAAE,KAAK;MACjBC,SAAS,EAAE,KAAK;MAChBC,UAAU,EAAE,KAAK;MACjBC,aAAa,EAAE;IACjB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,eAAeA,CAAA,EAAG;MAChB,OAAO;QACLC,eAAe,EAAE,OAAOX,UAAU,GAAG;QACrCY,cAAc,EAAE,OAAO;QACvBC,kBAAkB,EAAE,QAAQ;QAC5BC,gBAAgB,EAAE,WAAW;QAC7BC,KAAK,EAAE,MAAM;QACbC,MAAM,EAAE;MACV,CAAC;IACH;EACF,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,KAAKA,CAAA,EAAG;MACZ,IAAI,CAACZ,SAAQ,GAAI,IAAI;MACrB,IAAI,CAACC,UAAS,GAAI,KAAK;MACvB,IAAI,CAACC,aAAY,GAAI,KAAK;MAC1B,IAAI;QACF,MAAMW,QAAO,GAAI,MAAMpB,KAAK,CAACqB,IAAI,CAAC,sCAAsC,EAAE;UACxEjB,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC,CAAC;QAEF,MAAMiB,KAAI,GAAIF,QAAQ,CAACjB,IAAI,CAACmB,KAAK;QACjC,MAAMC,QAAO,GAAIH,QAAQ,CAACjB,IAAI,CAACoB,QAAQ;QAEvCC,YAAY,CAACC,OAAO,CAAC,OAAO,EAAEH,KAAK,CAAC;QACpCE,YAAY,CAACC,OAAO,CAAC,UAAU,EAAEF,QAAQ,CAAC;QAE1C,IAAI,IAAI,CAACjB,UAAU,EAAE;UACnBkB,YAAY,CAACC,OAAO,CAAC,YAAY,EAAE,IAAI,CAAC;QAC1C,OAAO;UACLD,YAAY,CAACE,UAAU,CAAC,YAAY,CAAC;QACvC;QAEA,IAAI,CAACC,KAAK,CAACL,KAAI,GAAIA,KAAK;QACxB,IAAI,CAACK,KAAK,CAACJ,QAAO,GAAIA,QAAQ;QAE9B,IAAI,CAACK,OAAO,CAACC,IAAI,CAAC,YAAY,CAAC;MACjC,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,cAAc,EAAEA,KAAK,CAAC;QACpC,IAAIA,KAAK,CAACV,QAAO,IAAKU,KAAK,CAACV,QAAQ,CAACY,MAAK,KAAM,GAAG,EAAE;UACnD,IAAI,CAACvB,aAAY,GAAI,IAAI;UACzBwB,KAAK,CAAC,8BAA8B,CAAC;QACvC,OAAO;UACLA,KAAK,CAA