{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      events: [],\n      searchQuery: '',\n      startDate: '',\n      endDate: ''\n    };\n  },\n  created() {\n    this.fetchEvents();\n  },\n  methods: {\n    fetchEvents() {\n      const params = {\n        name: this.searchQuery,\n        startDate: this.startDate,\n        endDate: this.endDate\n      };\n      axios.get(\"http://localhost:3000/api/events\", {\n        params,\n        headers: {\n          Authorization: `Bearer ${localStorage.getItem(\"token\")}`\n        }\n      }).then(response => {\n        this.events = response.data;\n      }).catch(error => {\n        console.error(\"Error fetching events:\", error);\n      });\n    },\n    clearFilters() {\n      this.searchQuery = \"\";\n      this.startDate = \"\";\n      this.endDate = \"\";\n      this.fetchEvents();\n    },\n    formatDate(dateString) {\n      const date = new Date(dateString);\n      return date.toISOString().split(\"T\")[0];\n    },\n    scrollLeft() {\n      const container = this.$el.querySelector(\".event-list-container\");\n      container.scrollLeft -= 300; // Adjust the scroll amount as needed\n    },\n    scrollRight() {\n      const container = this.$el.querySelector(\".event-list-container\");\n      container.scrollLeft += 300; // Adjust the scroll amount as needed\n    }\n  }\n};","map":{"version":3,"names":["axios","data","events","searchQuery","startDate","endDate","created","fetchEvents","methods","params","name","get","headers","Authorization","localStorage","getItem","then","response","catch","error","console","clearFilters","formatDate","dateString","date","Date","toISOString","split","scrollLeft","container","$el","querySelector","scrollRight"],"sources":["/Users/siddarthbandi/Desktop/SID/project/event-management-platform/event-management-frontend/src/components/EventListPublic.vue"],"sourcesContent":["<template>\n  <div class=\"container mt-4\">\n    <h2 class=\"text-center mb-4\">Event List</h2>\n\n    <!-- Search Bar -->\n    <div class=\"mb-3\">\n      <input \n        v-model=\"searchQuery\" \n        class=\"form-control\" \n        placeholder=\"Search by name\" \n        @input=\"fetchEvents\" \n      />\n    </div>\n\n    <!-- Date Range Filter -->\n    <div class=\"d-flex justify-content-between mb-3\">\n      <div>\n        <label for=\"startDate\">Start Date:</label>\n        <input \n          type=\"date\" \n          v-model=\"startDate\" \n          class=\"form-control\" \n          @change=\"fetchEvents\" \n        />\n      </div>\n      <div>\n        <label for=\"endDate\">End Date:</label>\n        <input \n          type=\"date\" \n          v-model=\"endDate\" \n          class=\"form-control\" \n          @change=\"fetchEvents\" \n        />\n      </div>\n    </div>\n\n    <!-- Clear Filters Button -->\n    <button class=\"btn btn-secondary mb-4\" @click=\"clearFilters\">Clear Filters</button>\n\n    <!-- Horizontal Scroll with Buttons -->\n    <div class=\"d-flex align-items-center\">\n      <button class=\"btn btn-primary scroll-btn\" @click=\"scrollLeft\">❮</button>\n      <div class=\"event-list-container\">\n        <div \n          v-for=\"event in events\" \n          :key=\"event.id\" \n          class=\"card h-100 event-card\"\n        >\n          <img \n            :src=\"event.image ? `http://localhost:3000${event.image}` : 'http://localhost:3000/uploads/default-image.jpg'\" \n            alt=\"Event Image\" \n            class=\"card-img-top\"\n          />\n          <div class=\"card-body\">\n            <h5 class=\"card-title\">{{ event.name }}</h5>\n            <p class=\"card-text\"><strong>Date:</strong> {{ formatDate(event.date) }}</p>\n            <router-link \n              :to=\"{ name: 'event-details-public', params: { id: event.id } }\" \n              class=\"btn btn-primary mt-3\"\n            >\n              View Details\n            </router-link>\n          </div>\n        </div>\n      </div>\n      <button class=\"btn btn-primary scroll-btn\" @click=\"scrollRight\">❯</button>\n    </div>\n\n    \n  </div>\n</template>\n\n\n\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  data() {\n    return {\n      events: [],\n      searchQuery: '',\n      startDate: '',\n      endDate: ''\n    };\n  },\n  created() {\n    this.fetchEvents();\n  },\n  methods: {\n    fetchEvents() {\n      const params = {\n        name: this.searchQuery,\n        startDate: this.startDate,\n        endDate: this.endDate,\n      };\n      axios\n        .get(\"http://localhost:3000/api/events\", {\n          params,\n          headers: {\n            Authorization: `Bearer ${localStorage.getItem(\"token\")}`,\n          },\n        })\n        .then((response) => {\n          this.events = response.data;\n        })\n        .catch((error) => {\n          console.error(\"Error fetching events:\", error);\n        });\n    },\n    clearFilters() {\n      this.searchQuery = \"\";\n      this.startDate = \"\";\n      this.endDate = \"\";\n      this.fetchEvents();\n    },\n    formatDate(dateString) {\n      const date = new Date(dateString);\n      return date.toISOString().split(\"T\")[0];\n    },\n    scrollLeft() {\n      const container = this.$el.querySelector(\".event-list-container\");\n      container.scrollLeft -= 300; // Adjust the scroll amount as needed\n    },\n    scrollRight() {\n      const container = this.$el.querySelector(\".event-list-container\");\n      container.scrollLeft += 300; // Adjust the scroll amount as needed\n    },\n  },\n\n};\n</script>\n\n<style scoped>\n.event-list-container {\n  display: flex;\n  overflow-x: hidden; /* Hide the scrollbar */\n  gap: 16px;\n  scroll-behavior: smooth; /* Smooth scrolling */\n  width: 100%;\n}\n\n.event-card {\n  flex: 0 0 auto;\n  width: 300px;\n}\n\n.scroll-btn {\n  background-color: #007bff;\n  color: white;\n  border: none;\n  border-radius: 50%;\n  height: 50px;\n  width: 50px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  cursor: pointer;\n  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\n}\n\n.scroll-btn:focus {\n  outline: none;\n}\n\n.scroll-btn:hover {\n  background-color: #0056b3;\n}\n\n.card-img-top {\n  height: 200px;\n  object-fit: cover;\n  border-radius: 8px; /* Adjust this as needed */\n  margin: 50px; /* Adds space around the image */\n}\n\n\n.card-body {\n  padding: 15px;\n}\n\n.card-title {\n  font-size: 1.25rem;\n  margin-bottom: 10px;\n}\n\n.btn {\n  width: 10%;\n}\n</style>\n"],"mappings":"AA4EA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,EAAE;MACVC,WAAW,EAAE,EAAE;MACfC,SAAS,EAAE,EAAE;MACbC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,WAAW,CAAC,CAAC;EACpB,CAAC;EACDC,OAAO,EAAE;IACPD,WAAWA,CAAA,EAAG;MACZ,MAAME,MAAK,GAAI;QACbC,IAAI,EAAE,IAAI,CAACP,WAAW;QACtBC,SAAS,EAAE,IAAI,CAACA,SAAS;QACzBC,OAAO,EAAE,IAAI,CAACA;MAChB,CAAC;MACDL,KAAI,CACDW,GAAG,CAAC,kCAAkC,EAAE;QACvCF,MAAM;QACNG,OAAO,EAAE;UACPC,aAAa,EAAE,UAAUC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;QACxD;MACF,CAAC,EACAC,IAAI,CAAEC,QAAQ,IAAK;QAClB,IAAI,CAACf,MAAK,GAAIe,QAAQ,CAAChB,IAAI;MAC7B,CAAC,EACAiB,KAAK,CAAEC,KAAK,IAAK;QAChBC,OAAO,CAACD,KAAK,CAAC,wBAAwB,EAAEA,KAAK,CAAC;MAChD,CAAC,CAAC;IACN,CAAC;IACDE,YAAYA,CAAA,EAAG;MACb,IAAI,CAAClB,WAAU,GAAI,EAAE;MACrB,IAAI,CAACC,SAAQ,GAAI,EAAE;MACnB,IAAI,CAACC,OAAM,GAAI,EAAE;MACjB,IAAI,CAACE,WAAW,CAAC,CAAC;IACpB,CAAC;IACDe,UAAUA,CAACC,UAAU,EAAE;MACrB,MAAMC,IAAG,GAAI,IAAIC,IAAI,CAACF,UAAU,CAAC;MACjC,OAAOC,IAAI,CAACE,WAAW,CAAC,CAAC,CAACC,KAAK,CAAC,GAAG,CAAC,CAAC,CAAC,CAAC;IACzC,CAAC;IACDC,UAAUA,CAAA,EAAG;MACX,MAAMC,SAAQ,GAAI,IAAI,CAACC,GAAG,CAACC,aAAa,CAAC,uBAAuB,CAAC;MACjEF,SAAS,CAACD,UAAS,IAAK,GAAG,EAAE;IAC/B,CAAC;IACDI,WAAWA,CAAA,EAAG;MACZ,MAAMH,SAAQ,GAAI,IAAI,CAACC,GAAG,CAACC,aAAa,CAAC,uBAAuB,CAAC;MACjEF,SAAS,CAACD,UAAS,IAAK,GAAG,EAAE;IAC/B;EACF;AAEF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}