{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      events: [] // Store the list of events\n    };\n  },\n  created() {\n    // Fetch all events when the component is created\n    axios.get('http://localhost:3000/api/events').then(response => {\n      this.events = response.data;\n    }).catch(error => {\n      console.error('Error fetching events:', error);\n    });\n  },\n  methods: {\n    formatDate(dateString) {\n      const date = new Date(dateString);\n      return date.toISOString().split('T')[0]; // Format the date as YYYY-MM-DD\n    },\n    editEvent(eventId) {\n      this.$router.push({\n        name: 'edit-event',\n        params: {\n          id: eventId\n        }\n      });\n    },\n    deleteEvent(eventId) {\n      if (confirm('Are you sure you want to delete this event?')) {\n        axios.delete(`http://localhost:3000/api/events/${eventId}`).then(() => {\n          alert('Event deleted successfully!');\n          this.events = this.events.filter(event => event.id !== eventId); // Remove deleted event from list\n        }).catch(error => {\n          console.error('Error deleting event:', error);\n        });\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","events","created","get","then","response","catch","error","console","methods","formatDate","dateString","date","Date","toISOString","split","editEvent","eventId","$router","push","name","params","id","deleteEvent","confirm","delete","alert","filter","event"],"sources":["/Users/siddarthbandi/Desktop/SID/project/event-management-platform/event-management-frontend/src/components/EventList.vue"],"sourcesContent":["<template>\n  <ul>\n    <li v-for=\"event in events\" :key=\"event.id\">\n      {{ event.name }} - {{ formatDate(event.date) }}\n        <button @click=\"editEvent(event.id)\">Edit</button> <!-- The method is properly attached here -->\n        <button @click=\"deleteEvent(event.id)\">Delete</button> <!-- Delete button -->\n\n    </li>\n  </ul>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  data() {\n    return {\n      events: []  // Store the list of events\n    };\n  },\n  created() {\n    // Fetch all events when the component is created\n    axios.get('http://localhost:3000/api/events')\n      .then(response => {\n        this.events = response.data;\n      })\n      .catch(error => {\n        console.error('Error fetching events:', error);\n      });\n  },\n   methods: {\n    formatDate(dateString) {\n      const date = new Date(dateString);\n      return date.toISOString().split('T')[0];  // Format the date as YYYY-MM-DD\n    },\n    editEvent(eventId) {\n      this.$router.push({ name: 'edit-event', params: { id: eventId } });\n    },\n    deleteEvent(eventId) {\n      if (confirm('Are you sure you want to delete this event?')) {\n        axios.delete(`http://localhost:3000/api/events/${eventId}`)\n          .then(() => {\n            alert('Event deleted successfully!');\n            this.events = this.events.filter(event => event.id !== eventId); // Remove deleted event from list\n          })\n          .catch(error => {\n            console.error('Error deleting event:', error);\n          });\n      }\n    }\n  }\n};\n</script>\n"],"mappings":";AAYA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,EAAC,CAAG;IACd,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR;IACAH,KAAK,CAACI,GAAG,CAAC,kCAAkC,EACzCC,IAAI,CAACC,QAAO,IAAK;MAChB,IAAI,CAACJ,MAAK,GAAII,QAAQ,CAACL,IAAI;IAC7B,CAAC,EACAM,KAAK,CAACC,KAAI,IAAK;MACdC,OAAO,CAACD,KAAK,CAAC,wBAAwB,EAAEA,KAAK,CAAC;IAChD,CAAC,CAAC;EACN,CAAC;EACAE,OAAO,EAAE;IACRC,UAAUA,CAACC,UAAU,EAAE;MACrB,MAAMC,IAAG,GAAI,IAAIC,IAAI,CAACF,UAAU,CAAC;MACjC,OAAOC,IAAI,CAACE,WAAW,CAAC,CAAC,CAACC,KAAK,CAAC,GAAG,CAAC,CAAC,CAAC,CAAC,EAAG;IAC5C,CAAC;IACDC,SAASA,CAACC,OAAO,EAAE;MACjB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;QAAEC,IAAI,EAAE,YAAY;QAAEC,MAAM,EAAE;UAAEC,EAAE,EAAE