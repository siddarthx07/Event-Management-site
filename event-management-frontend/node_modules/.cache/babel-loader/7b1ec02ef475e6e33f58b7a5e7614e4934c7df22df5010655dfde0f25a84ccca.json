 </ul>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  data() {\n    return {\n      events: []  // Store the list of events\n    };\n  },\n  created() {\n    // Fetch all events when the component is created\n    axios.get('http://localhost:3000/api/events')\n      .then(response => {\n        this.events = response.data;\n      })\n      .catch(error => {\n        console.error('Error fetching events:', error);\n      });\n  },\n  methods: {\n    formatDate(dateString) {\n      const date = new Date(dateString);\n      return date.toISOString().split('T')[0];  // Formats the date as YYYY-MM-DD\n    },\n    editEvent(eventId) {\n      // Use $router.push to navigate to the edit page for the selected event\n      console.log(`Navigating to edit event with ID: ${eventId}`);\n      this.$router.push({ name: 'edit-event', params: { id: eventId } });\n    }\n    editEvent(eventId) {\n      this.$router.push({ name: 'edit-event', params: { id: eventId } });\n    },\n    deleteEvent(eventId) {\n      if (confirm('Are you sure you want to delete this event?')) {\n        axios.delete(`http://localhost:3000/api/events/${eventId}`)\n          .then(() => {\n            alert('Event deleted successfully!');\n            this.events = this.events.filter(event => event.id !== eventId); // Remove deleted event from list\n          })\n          .catch(error => {\n            console.error('Error deleting event:', error);\n          });\n      }\n  }\n};\n</script>\n"],"mappings":";mBAAA;mBAAA;;uBACEA,mBAAA,CAOK,c,kBANHA,mBAAA,CAKKC,SAAA,QAPTC,WAAA,CAEwBC,IAAA,CAAAC,MAAM,EAAfC,KAAK;yBAAhBL,mBAAA,CAKK;MALwBM,GAAG,EAAED,KAAK,CAACE;QAF5CC,gBAAA,CAAAC,gBAAA,CAGSJ,KAAK,CAACK,IAAI,IAAG,KAAG,GAAAD,gBAAA,CAAGN,IAAA,CAAAQ,UAAU,CAACN,KAAK,CAACO,IAAI,KAAI,GAC7C,iBAAAC,mBAAA,CAAkD;MAAzCC,OAAK,EAAAC,MAAA,IAAEZ,IAAA,CAAAa,SAAS,CAACX,KAAK,CAACE,EAAE;OAAG,MAAI,iBAJjDU,UAAA,GAI2DC,mBAAA,0CAA6C,EAChGL,mBAAA,CAAsD;MAA7CC,OAAK,EAAAC,MAAA,IAAEZ,IAAA,CAAAgB,WAAW,CAACd,KAAK,