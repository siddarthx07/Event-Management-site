rom 'axios';\n\nexport default {\n  name: 'LoginPage',\n  data() {\n    return {\n      email: '',\n      password: ''\n    };\n  },\n  methods: {\n    async login() {\n      try {\n        const response = await axios.post('http://localhost:3000/api/auth/login', {\n          email: this.email,\n          password: this.password\n        });\n\n        const { token, username } = response.data;\n\n        console.log('Token:', token);\n\n        // Store token and username in localStorage\n        localStorage.setItem('token', token);\n        localStorage.setItem('username', username);\n\n        // Update Vue's reactive `token` and `username` data\n        this.$root.token = token;        // Ensure Vue's reactivity kicks in\n        this.$root.username = username;  // Update username reactively\n\n        alert('Login successful!');\n        this.$router.push('/dashboard'); // Redirect to dashboard or home page\n      } catch (error) {\n        console.error('Login error:', error);\n        alert('Login failed, please try again.');\n      }\n    }\n  }\n};\n</script>\n\n<style scoped>\n/* Overall container styling */\n.login-container {\n  max-width: 400px;\n  margin: 100px auto;\n  padding: 40px 30px;\n  background-color: #ffffff;\n  border: 1px solid #e0e0e0;\n  border-radius: 10px;\n  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\n}\n\n/* Title styling */\n.login-title {\n  text-align: center;\n  margin-bottom: 30px;\n  font-size: 28px;\n  color: #333333;\n}\n\n/* Form styling */\n.login-form .form-group {\n  margin-bottom: 20px;\n}\n\n.login-form label {\n  display: block;\n  margin-bottom: 8px;\n  font-weight: 500;\n  color: #555555;\n}\n\n.login-form input {\n  width: 100%;\n  padding: 12px 15px;\n  border: 1px solid #cccccc;\n  border-radius: 5px;\n  font-size: 16px;\n  transition: border-color 0.3s;\n}\n\n.login-form input:focus {\n  border-color: #007BFF;\n  outline: none;\n  box-shadow: 0 0 5px rgba(0, 123, 255, 0.2);\n}\n\n/* Button styling */\n.btn {\n  width: 100%;\n  padding: 12px 0;\n  background-color: #007BFF;\n  border: none;\n  border-radius: 5px;\n  color: #ffffff;\n  font-size: 18px;\n  cursor: pointer;\n  transition: background-color 0.3s;\n}\n\n.btn:hover {\n  background-color: #0056b3;\n}\n\n/* Signup link styling */\n.signup-link {\n  text-align: center;\n  margin-top: 20px;\n  color: #666666;\n  font-size: 14px;\n}\n\n.signup-link a {\n  color: #007BFF;\n  text-decoration: none;\n  font-weight: 500;\n}\n\n.signup-link a:hover {\n  text-decoration: underline;\n}\n\n/* Responsive Design */\n@media (max-width: 480px) {\n  .login-container {\n    padding: 30px 20px;\n    margin: 60px auto;\n  }\n\n  .login-title {\n    font-size: 24px;\n  }\n\n  .btn {\n    font-size: 16px;\n  }\n}\n</style>\n"],"mappings":";;EACOA,KAAK,EAAC;AAAiB;;EAGnBA,KAAK,EAAC;AAAY;;EAUlBA,KAAK,EAAC;AAAY;;EAYtBA,KAAK,EAAC;AAAa;;;uBAzBxBC,mBAAA,CA4BM,OA5BNC,UA4BM,G,0BA3BJC,mBAAA,CAAoC;IAAhCH,KAAK,EAAC;EAAa,GAAC,SAAO,sBAC/BG,mBAAA,CAsBO;IAtBAC,QAAM,EAAAC,MAAA,QAAAA,MAAA,MAHjBC,cAAA,KAAAC,IAAA,KAG2BC,QAAA,CAAAC,KAAA,IAAAD,QAAA,CAAAC,KAAA,IAAAF,IAAA,CAAK;IAAEP,KAAK,EAAC;MAClCG,mBAAA,CASM,OATNO,UASM,G,0BARJP,mBAAA,CAAgC;IAAzBQ,GAAG,EAAC;EAAO,GAAC,OAAK,sB,gBACxBR,mBAAA,CAME;IALAS,IAAI,EAAC,OAAO;IAPtB,uBAAAP,MAAA,QAAAA,MAAA,MAAAQ,MAAA,IAQmBC,KAAA,CAAAC,KAAK,GAAAF,MAAA;IACdG,EAAE,EAAC,OAAO;IACVC,WAAW,EAAC,kBAAkB;IAC9BC,QAAQ,EAAR;iDAHSJ,KAAA,CAAAC,KAAK,E,KAMlBZ,mBAAA,CASM,OATNgB,UASM,G,0BARJhB,mBAAA,CAAsC;IAA/BQ,GAAG,EAAC;EAAU,GAAC,UAAQ,sB,gBAC9BR,mBAAA,CAME;IALAS,IAAI,EAAC,UAAU;IAjBzB,uBAAAP,MAAA,QAAAA,MAAA,MAAAQ,MAAA,IAkBmBC,KAAA,CAAAM,QAAQ,GAAAP,MAAA;IACjBG,EAAE,EAAC,UAAU;IACbC,WAAW,EAAC,qBAAqB;IACjCC,QAAQ,EAAR;iDAHSJ,KAAA,CAAAM,QAAQ,E,+BAMrBjB,mBAAA,CAAgD;IAAxCS,IAAI,EAAC,QAAQ;IAACZ,KAAK,EAAC;KAAM,OAAK,qB,4BAEzCG,mBAAA,CAEI,KAFJkB,UAEI,G,0BA5BRC,gBAAA,CA0B2B,0BACE,IAAAC,YAAA,CAAiDC,sBAAA;IAApCC,EAAE,EAAC;EAAW;IA3BxDC,OAAA,EAAAC,QAAA,CA2ByD,MAAOtB,MAAA,QAAAA,MAAA,OA3BhEiB,gBAAA,CA2ByD,SAAO,E;IA3BhEM,CAAA","ignoreList":[]},"metadata":{},"sour