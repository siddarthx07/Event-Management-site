{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      events: [],\n      // Store the list of events\n      errorMessage: '' // For any error messages while fetching events\n    };\n  },\n  created() {\n    // Fetch all events when the component is created\n    axios.get('http://localhost:3000/api/events').then(response => {\n      this.events = response.data; // Store the fetched events\n    }).catch(error => {\n      console.error('Error fetching events:', error);\n      this.errorMessage = 'Failed to load events.';\n    });\n  },\n  methods: {\n    formatDate(dateString) {\n      const date = new Date(dateString);\n      return date.toISOString().split('T')[0]; // Formats the date as YYYY-MM-DD\n    },\n    editEvent(eventId) {\n      // Use $router.push to navigate to the edit page for the selected event\n      console.log(`Navigating to edit event with ID: ${eventId}`);\n      this.$router.push({\n        name: 'edit-event',\n        params: {\n          id: eventId\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","data","events","errorMessage","created","get","then","response","catch","error","console","methods","formatDate","dateString","date","Date","toISOString","split","editEvent","eventId","log","$router","push","name","params","id"],"sources":["/Users/siddarthbandi/Desktop/SID/project/event-management-platform/event-management-frontend/src/components/EventList.vue"],"sourcesContent":["<template>\n  <div>\n    <h2>Event List</h2>\n    <ul v-if=\"events.length\">\n      <li v-for=\"event in events\" :key=\"event.id\">\n        {{ event.name }} - {{ formatDate(event.date) }}\n        <button @click=\"editEvent(event.id)\">Edit</button> <!-- Edit button to navigate to the edit page -->\n      </li>\n    </ul>\n    <p v-else>No events available.</p>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  data() {\n    return {\n      events: [], // Store the list of events\n      errorMessage: '' // For any error messages while fetching events\n    };\n  },\n  created() {\n    // Fetch all events when the component is created\n    axios.get('http://localhost:3000/api/events')\n      .then(response => {\n        this.events = response.data; // Store the fetched events\n      })\n      .catch(error => {\n        console.error('Error fetching events:', error);\n        this.errorMessage = 'Failed to load events.';\n      });\n  },\n  methods: {\n    formatDate(dateString) {\n      const date = new Date(dateString);\n      return date.toISOString().split('T')[0]; // Formats the date as YYYY-MM-DD\n    },\n    editEvent(eventId) {\n      // Use $router.push to navigate to the edit page for the selected event\n      console.log(`Navigating to edit event with ID: ${eventId}`);\n      this.$router.push({ name: 'edit-event', params: { id: eventId } });\n    }\n  }\n};\n</script>\n"],"mappings":";AAcA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,EAAE;MAAE;MACZC,YAAY,EAAE,EAAC,CAAE;IACnB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR;IACAJ,KAAK,CAACK,GAAG,CAAC,kCAAkC,EACzCC,IAAI,CAACC,QAAO,IAAK;MAChB,IAAI,CAACL,MAAK,GAAIK,QAAQ,CAACN,IAAI,EAAE;IAC/B,CAAC,EACAO,KAAK,CAACC,KAAI,IAAK;MACdC,OAAO,CAACD,KAAK,CAAC,wBAAwB,EAAEA,KAAK,CAAC;MAC9C,IAAI,CAACN,YAAW,GAAI,wBAAwB;IAC9C,CAAC,CAAC;EACN,CAAC;EACDQ,OAAO,EAAE;IACPC,UAAUA,CAACC,UAAU,EAAE;MACrB,MAAMC,IAAG,GAAI,IAAIC,IAAI,CAACF,UAAU,CAAC;MACjC,OAAOC,IAAI,CAACE,WAAW,CAAC,CAAC,CAACC,KAAK,CAAC,GAAG,CAAC,CAAC,CAAC,CAAC,EAAE;IAC3C,CAAC;IACDC,SAASA,CAACC,OAAO,EAAE;MACjB;MACAT,OAAO,CAACU,GAAG,CAAC,qCAAqCD,OAAO,EAAE,CAAC;MAC3D,IAAI,CAACE,OAAO,CAACC,IAAI,CAAC;QAAEC,IAAI,EAAE,YAAY;QAAEC,MAAM,EAAE;UAAEC,EAAE,EAAEN;QAAQ;MAAE,CAAC,CAAC;IACpE;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}