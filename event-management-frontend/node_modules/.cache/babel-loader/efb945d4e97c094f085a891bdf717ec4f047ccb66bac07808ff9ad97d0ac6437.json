{\n        this.emailError = true;\n      }\n      if (!this.password.trim()) {\n        this.passwordError = true;\n      }\n      if (this.emailError || this.passwordError) {\n        this.isLoading = false;\n        return;\n      }\n      try {\n        const response = await axios.post('http://localhost:3000/api/auth/login', {\n          email: this.email,\n          password: this.password\n        });\n        const {\n          token,\n          username\n        } = response.data;\n        console.log('Token:', token);\n\n        // Store token and username in localStorage\n        localStorage.setItem('token', token);\n        localStorage.setItem('username', username);\n\n        // Optionally handle 'Remember Me' functionality here\n\n        // Update Vue's reactive `token` and `username` data\n        this.$root.token = token; // Ensure Vue's reactivity kicks in\n        this.$root.username = username; // Update username reactively\n\n        this.loginSuccess = true;\n        setTimeout(() => {\n          this.$router.push('/dashboard'); // Redirect after 1.5 seconds\n        }, 1500);\n      } catch (error) {\n        console.error('Login error:', error);\n        if (error.response && error.response.status === 401) {\n          this.passwordError = true;\n          this.loginErrorMessage = 'Incorrect email or password.';\n        } else {\n          this.loginErrorMessage = 'Login failed, please try again.';\n        }\n      } finally {\n        this.isLoading = false;\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","backgroundImages","name","data","email","password","backgroundImage","isLoading","emailError","passwordError","computed","backgroundStyle","created","setRandomBackground","methods","randomIndex","Math","floor","random","length","console","log","login","trim","response","post","token","username","localStorage","setItem","$root","loginSuccess","setTimeout","$router","push","error","status","loginErrorMessage"],"sources":["/Users/siddarthbandi/Desktop/SID/project/event-management-platform/event-management-frontend/src/components/Login.vue"],"sourcesContent":["<!-- src/components/LoginPage.vue -->\n\n<template>\n  <div class=\"auth-container\" :style=\"backgroundStyle\">\n    <div class=\"overlay\">\n      <div class=\"auth-card\">\n        <h2 class=\"auth-title\">Sign In</h2>\n        <form @submit.prevent=\"login\" class=\"auth-form\">\n\n          <div class=\"form-group\" :class=\"{ 'has-error': emailError }\">\n            <label for=\"email\">Email</label>\n            <input\n              type=\"email\"\n              v-model=\"email\"\n              id=\"email\"\n              placeholder=\"Enter your email\"\n              required\n              class=\"auth-input\"\n              aria-label=\"Email Address\"\n            />\n            <span v-if=\"emailError\" class=\"error-message\">Invalid email address.</span>\n          </div>\n\n          <div class=\"form-group\" :class=\"{ 'has-error': passwordError }\">\n            <label for=\"password\">Password</label>\n            <input\n              type=\"password\"\n              v-model=\"password\"\n              id=\"password\"\n              placeholder=\"Enter your password\"\n              required\n              class=\"auth-input\"\n              aria-label=\"Password\"\n            />\n            <span v-if=\"passwordError\" class=\"error-message\">Incorrect password.</span>\n          </div>\n\n          <button type=\"submit\" class=\"auth-button\" :disabled=\"isLoading\">\n            <span v-if=\"!isLoading\">Login</span>\n            <span v-else class=\"spinner\"></span>\n          </button>\n        </form>\n        <p class=\"auth-link\">\n          Don't have an account?\n          <router-link to=\"/register\" class=\"link\">Sign Up</router-link>\n        </p>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\n// Define the background images using absolute paths\nconst backgroundImages = [\n  '/assets/events/pexels-bertellifotografia-2608517.jpg',\n  '/assets/events/pexels-bertellifotografia-29406512.jpg',\n  '/assets/events/pexels-ezequiel-alfonso-3234743-29347735.jpg',\n  '/assets/events/pexels-franco-monsalvo-252430633-29356373.jpg',\n  '/assets/events/pexels-icsa-833425-1709003.jpg',\n  '/assets/events/pexels-israyosoy-29436830.jpg',\n  '/assets/events/pexels-israyosoy-29436916.jpg',\n  '/assets/events/pexels-jonathanborba-29382709.jpg',\n  '/assets/events/pexels-juan-felipe-ramirez-312591454-29361381.jpg',\n  '/assets/events/pexels-osvaldosam-29367552.jpg',\n  '/assets/events/pexels-pixabay-433452.jpg',\n  '/assets/events/pexels-sandra-nanteza-532613123-29370058.jpg',\n  '/assets/events/pexels-sandra-nanteza-532613123-29370382.jpg',\n  '/assets/events/pexels-srivathsa-3760743-29345538.jpg',\n  '/assets/events/pexels-zhuhehuai-716276.jpg',\n  '/assets/events/photo-1492684223066-81342ee5ff30.jpeg',\n];\n\nexport default {\n  name: 'LoginPage',\n  data() {\n    return {\n      email: '',\n      password: '',\n      backgroundImage: '', // To store the selected background image\n      isLoading: false,\n      emailError: false,\n      passwordError: false,\n    };\n  },\n  computed: {\n    backgroundStyle() {\n      return {\n        backgroundImage: `url(${this.backgroundImage})`,\n      };\n    },\n  },\n  created() {\n    this.setRandomBackground();\n  },\n  methods: {\n    setRandomBackground() {\n      const randomIndex = Math.floor(Math.random() * backgroundImages.length);\n      this.backgroundImage = backgroundImages[randomIndex];\n      console.log('Selected Background Image:', this.backgroundImage);\n    },\n    async login() {\n      this.isLoading = true;\n      // Reset error states\n      this.emailError = false;\n      this.passwordError = false;\n\n      // Basic form validation\n      if (!this.email.trim()) {\n        this.emailError = true;\n      }\n      if (!this.password.trim()) {\n        this.passwordError = true;\n      }\n\n      if (this.emailError || this.passwordError) {\n        this.isLoading = false;\n        return;\n      }\n\n      try {\n        const response = await axios.post('http://localhost:3000/api/auth/login', {\n          email: this.email,\n          password: this.password,\n        });\n\n        const { token, username } = response.data;\n\n        console.log('Token:', token);\n\n        // Store token and username in localStorage\n        localStorage.setItem('token', token);\n        localStorage.setItem('username', username);\n\n        // Optionally handle 'Remember Me' functionality here\n\n        // Update Vue's reactive `token` and `username` data\n        this.$root.token = token; // Ensure Vue's reactivity kicks in\n        this.$root.username = username; // Update username reactively\n\n        this.loginSuccess = true;\n        setTimeout(() => {\n          this.$router.push('/dashboard'); // Redirect after 1.5 seconds\n        }, 1500);\n      } catch (error) {\n        console.error('Login error:', error);\n        if (error.response && error.response.status === 401) {\n          this.passwordError = true;\n          this.loginErrorMessage = 'Incorrect email or password.';\n        } else {\n          this.loginErrorMessage = 'Login failed, please try again.';\n        }\n      } finally {\n        this.isLoading = false;\n      }\n    }\n  }\n};\n</script>\n\n<style scoped>\n.auth-container {\n  min-height: 100vh;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  background-size: cover;\n  background-position: center;\n  background-repeat: no-repeat;\n}\n\n.overlay {\n  background: linear-gradient(\n    to bottom right,\n    rgba(0, 0, 0, 0.5),\n    rgba(0, 0, 0, 0.3)\n  ); /* Gradient overlay for better contrast */\n  width: 100%;\n  height: 100%;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n\n.auth-card {\n  background: rgba(255, 255, 255, 0.7); /* Semi-transparent white */\n  padding: 2rem;\n  border-radius: 0.5rem;\n  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\n  width: 100%;\n  max-width: 400px;\n  margin: 1rem;\n  transition: background-color 0.3s ease;\n}\n\n.auth-title {\n  font-size: 1.5rem;\n  font-weight: bold;\n  margin-bottom: 1.5rem;\n  text-align: center;\n  text-shadow: 1px 1px 2px rgba(0, 0, 0, 0.2); /* Enhances text readability */\n}\n\n.auth-form .form-group {\n  margin-bottom: 1rem;\n  position: relative;\n}\n\n.auth-form label {\n  display: block;\n  margin-bottom: 0.5rem;\n  font-weight: 500;\n  color: #555555;\n  text-shadow: 1px 1px 2px rgba(0, 0, 0, 0.2); /* Enhances label readability */\n}\n\n.auth-input {\n  width: 100%;\n  padding: 0.75rem 1rem;\n  border: 1px solid #ddd;\n  border-radius: 0.25rem;\n  transition: border-color 0.3s, box-shadow 0.3s;\n}\n\n.auth-input:focus {\n  border-color: #6c63ff;\n  outline: none;\n  box-shadow: 0 0 5px rgba(108, 99, 255, 0.5);\n}\n\n.has-error input {\n  border-color: #e74c3c;\n}\n\n.error-message {\n  color: #e74c3c;\n  font-size: 0.875rem;\n  position: absolute;\n  bottom: -1.25rem;\n  left: 0;\n}\n\n.auth-button {\n  width: 100%;\n  padding: 0.75rem;\n  background-color: #6c63ff;\n  color: #ffffff;\n  border: none;\n  border-radius: 0.25rem;\n  font-size: 1rem;\n  cursor: pointer;\n  transition: background-color 0.3s;\n}\n\n.auth-button:hover {\n  background-color: #574b90;\n}\n\n.auth-button:active {\n  background-color: #3f3070;\n}\n\n.auth-button:disabled {\n  background-color: #a9a9a9;\n  cursor: not-allowed;\n}\n\n.spinner {\n  border: 4px solid rgba(255, 255, 255, 0.3);\n  border-left-color: #ffffff;\n  height: 16px;\n  width: 16px;\n  border-radius: 50%;\n  display: inline-block;\n  animation: spin 1s linear infinite;\n  margin: 0 auto;\n}\n\n@keyframes spin {\n  to {\n    transform: rotate(360deg);\n  }\n}\n\n.auth-link {\n  text-align: center;\n  margin-top: 1rem;\n  color: #333333;\n  text-shadow: 1px 1px 2px rgba(0, 0, 0, 0.2); /* Enhances paragraph readability */\n}\n\n.auth-link a {\n  color: #6c63ff;\n  text-decoration: none;\n  font-weight: 500;\n}\n\n.auth-link a:hover {\n  text-decoration: underline;\n}\n\n/* Optional: Logo Styling */\n.logo {\n  width: 100px;\n  margin-bottom: 1rem;\n  display: block;\n  margin-left: auto;\n  margin-right: auto;\n}\n\n/* Responsive Design */\n@media (max-width: 480px) {\n  .auth-card {\n    padding: 1.5rem;\n  }\n\n  .auth-title {\n    font-size: 1.25rem;\n  }\n\n  .auth-input {\n    padding: 0.6rem 0.8rem;\n  }\n\n  .auth-button {\n    padding: 0.5rem;\n    font-size: 0.9rem;\n  }\n}\n</style>\n"],"mappings":";AAoDA,OAAOA,KAAI,MAAO,OAAO;;AAEzB;AACA,MAAMC,gBAAe,GAAI,CACvB,sDAAsD,EACtD,uDAAuD,EACvD,6DAA6D,EAC7D,8DAA8D,EAC9D,+CAA+C,EAC/C,8CAA8C,EAC9C,8CAA8C,EAC9C,kDAAkD,EAClD,kEAAkE,EAClE,+CAA+C,EAC/C,0CAA0C,EAC1C,6DAA6D,EAC7D,6DAA6D,EAC7D,sDAAsD,EACtD,4CAA4C,EAC5C,sDAAsD,CACvD;AAED,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,eAAe,EAAE,EAAE;MAAE;MACrBC,SAAS,EAAE,KAAK;MAChBC,UAAU,EAAE,KAAK;MACjBC,aAAa,EAAE;IACjB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,eAAeA,CAAA,EAAG;MAChB,OAAO;QACLL,eAAe,EAAE,OAAO,IAAI,CAACA,eAAe;MAC9C,CAAC;IACH;EACF,CAAC;EACDM,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,mBAAmB,CAAC,CAAC;EAC5B,CAAC;EACDC,OAAO,EAAE;IACPD,mBAAmBA,CAAA,EAAG;MACpB,MAAME,WAAU,GAAIC,IAAI,CAACC,KAAK,CAACD,IAAI,CAACE,MAAM,CAAC,IAAIjB,gBAAgB,CAACkB,MAAM,CAAC;MACvE,IAAI,CAACb,eAAc,GAAIL,gBAAgB,CAACc,WAAW,CAAC;MACpDK,OAAO,CAACC,GAAG,CAAC,4BAA4B,EAAE,IAAI,CAACf,eAAe,CAAC;IACjE,CAAC;IACD,MAAMgB,KAAKA,CAAA,EAAG;MACZ,IAAI,CAACf,SAAQ,GAAI,IAAI;MACrB;MACA,IAAI,CAACC,UAAS,GAAI,KAAK;MACvB,IAAI,CAACC,aAAY,GAAI,KAAK;;MAE1B;MACA,IAAI,CAAC,IAAI,CAACL,KAAK,CAACmB,IAAI,CAAC,CAAC,EAAE;QACtB,IAAI,CAACf,UAAS,GAAI,IAAI;MACxB;MACA,IAAI,CAAC,IAAI,CAACH,QAAQ,CAACkB,IAAI,CAAC,CAAC,EAAE;QACzB,IAAI,CAACd,aAAY,GAAI,IAAI;MAC3B;MAEA,IAAI,IAAI,CAACD,UAAS,IAAK,IAAI,CAACC,aAAa,EAAE;QACzC,IAAI,CAACF,SAAQ,GAAI,KAAK;QACtB;MACF;MAEA,IAAI;QACF,MAAMiB,QAAO,GAAI,MAAMxB,KA