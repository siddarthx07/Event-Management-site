{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      username: localStorage.getItem(\"username\"),\n      role: localStorage.getItem(\"role\") // Get the role from localStorage\n    };\n  },\n  methods: {\n    becomeOrganizer() {\n      axios.put(\"http://localhost:3000/api/users/role/organizer\", {}, {\n        headers: {\n          Authorization: `Bearer ${localStorage.getItem(\"token\")}`\n        }\n      }).then(response => {\n        localStorage.setItem(\"role\", response.data.role);\n        this.role = response.data.role;\n        this.$router.push(\"/organize\");\n      }).catch(error => {\n        console.error(\"Error updating role:\", error);\n        alert(\"Failed to become an organizer.\");\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","data","username","localStorage","getItem","role","methods","becomeOrganizer","put","headers","Authorization","then","response","setItem","$router","push","catch","error","console","alert"],"sources":["/Users/siddarthbandi/Desktop/SID/project/event-management-platform/event-management-frontend/src/components/UserDashboard.vue"],"sourcesContent":["<template>\n  <div class=\"dashboard\">\n    <h2 class=\"welcome-message\">Welcome, {{ username }}!</h2>\n    <p class=\"instructions\">Select an option to get started</p>\n\n    <div class=\"cards-container\">\n      <!-- Left Card: View Events -->\n      <router-link to=\"/\" class=\"card\">\n        <div class=\"card-content\">\n          <i class=\"fas fa-eye card-icon\"></i>\n          <p class=\"card-title\">View Events</p>\n        </div>\n      </router-link>\n\n      <!-- Right Card: Organize Events -->\n      <button @click=\"becomeOrganizer\" class=\"card\">\n        <div class=\"card-content\">\n          <i class=\"fas fa-edit card-icon\"></i>\n          <p class=\"card-title\">Organize Events</p>\n        </div>\n      </button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  data() {\n    return {\n      username: localStorage.getItem(\"username\"),\n      role: localStorage.getItem(\"role\"), // Get the role from localStorage\n    };\n  },\n  methods: {\n    becomeOrganizer() {\n      axios\n        .put(\n          \"http://localhost:3000/api/users/role/organizer\",\n          {},\n          {\n            headers: {\n              Authorization: `Bearer ${localStorage.getItem(\"token\")}`,\n            },\n          }\n        )\n        .then((response) => {\n          localStorage.setItem(\"role\", response.data.role);\n          this.role = response.data.role;\n          this.$router.push(\"/organize\");\n        })\n        .catch((error) => {\n          console.error(\"Error updating role:\", error);\n          alert(\"Failed to become an organizer.\");\n        });\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* Dashboard Container */\n.dashboard {\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n  height: 100vh;\n  background-color: #f8f9fa; /* Light background */\n  padding: 20px;\n  text-align: center;\n}\n\n/* Welcome Message */\n.welcome-message {\n  font-size: 2rem;\n  font-weight: bold;\n  color: #333;\n  margin-bottom: 10px;\n}\n\n.instructions {\n  font-size: 1.25rem;\n  color: #555;\n  margin-bottom: 30px;\n}\n\n/* Cards Container: Divides the page into two halves */\n.cards-container {\n  display: flex;\n  justify-content: space-between;\n  width: 100%;\n  max-width: 800px;\n  gap: 2rem; /* Space between cards */\n}\n\n/* Card Styling */\n.card {\n  background: #ffffff;\n  flex: 1; /* Equal width for both cards */\n  max-width: 300px; /* Medium size card */\n  height: 250px; /* Medium height */\n  border-radius: 12px;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  flex-direction: column;\n  text-decoration: none;\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1); /* Subtle shadow */\n  transition: transform 0.2s ease, box-shadow 0.2s ease;\n  cursor: pointer;\n}\n\n.card:hover {\n  transform: translateY(-5px);\n  box-shadow: 0 6px 12px rgba(0, 0, 0, 0.2);\n}\n\n.card-content {\n  text-align: center;\n}\n\n.card-icon {\n  font-size: 3rem;\n  color: #6c63ff;\n  margin-bottom: 1rem;\n}\n\n.card-title {\n  font-size: 1.25rem;\n  font-weight: 600;\n  color: #333;\n}\n\n/* Responsive Adjustments */\n@media (max-width: 768px) {\n  .cards-container {\n    flex-direction: column;\n    gap: 1.5rem;\n  }\n\n  .card {\n    max-width: 100%; /* Full width on smaller screens */\n    height: 200px;\n  }\n}\n</style>\n"],"mappings":";AA0BA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAEC,YAAY,CAACC,OAAO,CAAC,UAAU,CAAC;MAC1CC,IAAI,EAAEF,YAAY,CAACC,OAAO,CAAC,MAAM,CAAC,CAAE;IACtC,CAAC;EACH,CAAC;EACDE,OAAO,EAAE;IACPC,eAAeA,CAAA,EAAG;MAChBP,KAAI,CACDQ,GAAG,CACF,gDAAgD,EAChD,CAAC,CAAC,EACF;QACEC,OAAO,EAAE;UACPC,aAAa,EAAE,UAAUP,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;QACxD;MACF,CACF,EACCO,IAAI,CAAEC,QAAQ,IAAK;QAClBT,YAAY,CAACU,OAAO,CAAC,MAAM,EAAED,QAAQ,CAACX,IAAI,CAACI,IAAI,CAAC;QAChD,IAAI,CAACA,IAAG,GAAIO,QAAQ,CAACX,IAAI,CAACI,IAAI;QAC9B,IAAI,CAACS,OAAO,CAACC,IAAI,CAAC,WAAW,CAAC;MAChC,CAAC,EACAC,KAAK,CAAEC,KAAK,IAAK;QAChBC,OAAO,CAACD,KAAK,CAAC,sBAAsB,EAAEA,KAAK,CAAC;QAC5CE,KAAK,CAAC,gCAAgC,CAAC;MACzC,CAAC,CAAC;IACN;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}