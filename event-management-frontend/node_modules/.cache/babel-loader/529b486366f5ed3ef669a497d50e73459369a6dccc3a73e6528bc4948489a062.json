       console.error(\"Error updating role:\", error);\n          alert(\"Failed to become an organizer.\");\n        });\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* Dashboard Container */\n.dashboard {\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n  height: 100vh;\n  background-color: #f8f9fa; /* Light background */\n  padding: 20px;\n  text-align: center;\n}\n\n/* Welcome Message */\n.welcome-message {\n  font-size: 2rem;\n  font-weight: bold;\n  color: #333;\n  margin-bottom: 10px;\n}\n\n.instructions {\n  font-size: 1.25rem;\n  color: #555;\n  margin-bottom: 30px;\n}\n\n/* Cards Container: Divides the page into two halves */\n.cards-container {\n  display: flex;\n  justify-content: space-between;\n  width: 100%;\n  max-width: 800px;\n  gap: 2rem; /* Space between cards */\n}\n\n/* Card Styling */\n.card {\n  background: #ffffff;\n  flex: 1; /* Equal width for both cards */\n  max-width: 300px; /* Medium size card */\n  height: 250px; /* Medium height */\n  border-radius: 12px;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  flex-direction: column;\n  text-decoration: none;\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1); /* Subtle shadow */\n  transition: transform 0.2s ease, box-shadow 0.2s ease;\n  cursor: pointer;\n}\n\n.card:hover {\n  transform: translateY(-5px);\n  box-shadow: 0 6px 12px rgba(0, 0, 0, 0.2);\n}\n\n.card-content {\n  text-align: center;\n}\n\n.card-icon {\n  font-size: 3rem;\n  color: #6c63ff;\n  margin-bottom: 1rem;\n}\n\n.card-title {\n  font-size: 1.25rem;\n  font-weight: 600;\n  color: #333;\n}\n\n/* Responsive Adjustments */\n@media (max-width: 768px) {\n  .cards-container {\n    flex-direction: column;\n    gap: 1.5rem;\n  }\n\n  .card {\n    max-width: 100%; /* Full width on smaller screens */\n    height: 200px;\n  }\n}\n</style>\n"],"mappings":";;EACOA,KAAK,EAAC;AAAW;;EAChBA,KAAK,EAAC;AAAiB;;EAGtBA,KAAK,EAAC;AAAiB;;;uBAJ9BC,mBAAA,CAqBM,OArBNC,UAqBM,GApBJ